let a = int(ask("Enter a number: "));
let b = int(ask("Enter another number: "));
let operation = ask("Enter an operation: ");
if (operation == "+") {
    print(a + b)
} else if (operation == "-") {
    print(a - b)
} else if (operation == "*") {
    print(a * b)
} else if (operation == "/") {
    print(a / b)
} else {
    print("Invalid operation")
}